<h1>The Jobs EJS Application</h1>
<% if (errors && errors.length > 0) { %>
    <% errors.forEach(function(err) { %>
      <div class="alert alert-danger"><%= err %></div>
    <% }) %>
<% } %>
<% if (info && info.length > 0) { %>
    <% info.forEach(function(msg) { %>
      <div class="alert alert-info"><%= msg %></div>
    <% }) %>
<% } %>
<hr />